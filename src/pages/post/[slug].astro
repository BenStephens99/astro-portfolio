--- 
import { getPosts } from '../../sanity.js'

export async function getStaticPaths() {

  const posts = await getPosts();

  return posts.map(({ slug, title }) => {
    return {
      params: { slug: slug.current },
      props: { title },
    };
  });
}

const { slug  } = Astro.params;
const { title } = Astro.props;
---

<h1>{title}</h1>